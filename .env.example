# Internet Archive API Configuration
# Copy this file to .env and fill in your actual credentials
# NEVER commit .env to version control

# Internet Archive API Credentials
# Obtain from: https://archive.org/account/s3.php
IA_ACCESS_KEY=your_access_key_here
IA_SECRET_KEY=your_secret_key_here

# API Version
IA_API_VERSION=1

# Optional: Custom endpoints (defaults shown)
# IA_BASE_URL=https://archive.org
# IA_AUTH_URL=https://archive.org/services/xauthn/
# IA_SEARCH_URL=https://archive.org/advancedsearch.php
# IA_METADATA_URL=https://archive.org/metadata/
